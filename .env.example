# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key
# For Vite, use these instead or alongside the above
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# OpenAI API Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key
# For Vite
VITE_OPENAI_API_KEY=your_openai_api_key

# Environment
REACT_APP_ENV=development
VITE_ENV=development

# App Configuration
VITE_APP_NAME=Buildera

# When deploying to production, make a copy of this file named .env
# and fill in the actual values. Do not commit your actual .env file to version control.
# For CI/CD pipelines, set these environment variables in your deployment platform. 